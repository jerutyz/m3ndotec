<h1>Listing encargados</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @encargados.each do |encargado| %>
  <tr>
    <td><%= encargado.nombre %></td>
    <td><%= encargado.email %></td>
    <td><%= link_to 'Show', encargado %></td>
    <td><%= link_to 'Edit', edit_encargado_path(encargado) %></td>
    <td><%= link_to 'Destroy', encargado, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Encargado', new_encargado_path %>
